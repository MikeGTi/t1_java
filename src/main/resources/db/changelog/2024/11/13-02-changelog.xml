<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1731512628933-6" author="mboychook">
        <dropForeignKeyConstraint baseTableName="account" constraintName="account_client_id_fkey"/>
    </changeSet>
    <changeSet id="1731512628933-3" author="mboychook">
        <addColumn tableName="account">
            <column name="cliend_id" type="BIGINT"/>
            <column name="frozen_amount" type="DECIMAL(19, 2)"/>
            <column name="status" type="SMALLINT"/>
        </addColumn>
    </changeSet>
    <changeSet id="1731512628933-7" author="mboychook">
        <dropColumn columnName="client_id" tableName="account"/>

        <dropColumn columnName="account_type" tableName="account"/>
    </changeSet>
    <changeSet id="1731512628933-8" author="mboychook">
        <dropSequence sequenceName="client_seq"/>
    </changeSet>
    <changeSet id="1731512628933-9" author="mboychook">
        <dropSequence sequenceName="data_source_error_log_seq"/>
    </changeSet>
    <changeSet id="1731512628933-10" author="mboychook">
        <dropSequence sequenceName="tbl_transaction_seq"/>
    </changeSet>
    <changeSet id="1731512628933-2" author="mboychook">
        <addColumn tableName="account">
            <column name="account_type" type="SMALLINT"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>